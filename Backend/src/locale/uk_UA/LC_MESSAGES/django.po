#: chat/conversation_utils.py:90 chat/conversation_utils.py:92
#: services/chat_bots.py:44
msgid "Hello there!"
msgstr "Привіт!"

#: chat/parse_incoming_message.py:46 chat/parse_incoming_message.py:61
msgid "Invalid conversation command"
msgstr "Недійсна команда бесіди"

#: chat/parse_incoming_message.py:50
msgid "Invalid map name"
msgstr "Недійсна назва карти"

#: chat/parse_incoming_message.py:52
msgid "Invalid powerup value"
msgstr "Недійсне значення бонусу"

#: chat/parse_incoming_message.py:59
#, python-brace-format
msgid "Game already exists: {link_game}"
msgstr "Гра вже існує: {link_game}"

#: chat/serializers.py:27
#, python-brace-format
msgid "@{0} has invited @{1} to play a friendly match: {2}"
msgstr "@{0} запросив(ла) @{1} на дружній матч: {2}"

#: chat/serializers.py:31
#, python-brace-format
msgid "@{0} has deleted the friendly match: {1}"
msgstr "@{0} видалив(ла) дружній матч: {1}"

#: chat/serializers.py:35
#, python-brace-format
msgid "@{0} has conceded the friendly match {1} and is therefore the loser"
msgstr "@{0} визнав(ла) поразку в дружньому матчі {1} і тому програв(ла)"

#: chat/serializers.py:39
#, python-brace-format
msgid "@1 lost their patience. Game {0} has been set to finished since the deadline has passed. They randomly decided a winner."
msgstr "@1 втратив(ла) терпіння. Гру {0} завершено, оскільки термін сплив. Переможця вибрано випадково."

#: chat/serializers.py:43
#, python-brace-format
msgid "User @{0} has sent a friend request to @{1}"
msgstr "Користувач @{0} надіслав(ла) запит на дружбу користувачу @{1}"

#: chat/serializers.py:47
#, python-brace-format
msgid "User @{0} has accepted the friend request from @{1}"
msgstr "Користувач @{0} прийняв(ла) запит на дружбу від @{1}"

#: chat/serializers.py:51
#, python-brace-format
msgid "User @{0} has canceled the friend request to @{1}"
msgstr "Користувач @{0} скасував(ла) запит на дружбу до @{1}"

#: chat/serializers.py:55
#, python-brace-format
msgid "User @{0} has rejected the friend request from @{1}"
msgstr "Користувач @{0} відхилив(ла) запит на дружбу від @{1}"

#: chat/serializers.py:59
#, python-brace-format
msgid "User @{0} has removed @{1} from their friends list"
msgstr "Користувач @{0} видалив(ла) @{1} зі списку друзів"

#: chat/serializers.py:63
#, python-brace-format
msgid "User @{0} has blocked @{1}"
msgstr "Користувач @{0} заблокував(ла) @{1}"

#: chat/serializers.py:67
#, python-brace-format
msgid "User @{0} has unblocked @{1}"
msgstr "Користувач @{0} розблокував(ла) @{1}"

#: chat/serializers.py:71
#, python-brace-format
msgid "User @{0} has started a conversation with @{1}"
msgstr "Користувач @{0} почав(ла) розмову з @{1}"

#: chat/serializers.py:75
#, python-brace-format
msgid "User @{0} has invited @{1} to the tournament: {2}"
msgstr "Користувач @{0} запросив(ла) @{1} на турнір: {2}"

#: chat/serializers.py:79
#, python-brace-format
msgid "User @{0} has left the tournament: {1}"
msgstr "Користувач @{0} покинув(ла) турнір: {1}"

#: chat/serializers.py:83
#, python-brace-format
msgid "User @{0} has joined the tournament: {1}"
msgstr "Користувач @{0} приєднався(лась) до турніру: {1}"

#: chat/serializers.py:87
#, python-brace-format
msgid "The tournament: {0} has started"
msgstr "Турнір {0} розпочався"

#: chat/serializers.py:91
#, python-brace-format
msgid "The tournament: {0} has been deleted by the creator"
msgstr "Турнір {0} був видалений творцем"

#: chat/serializers.py:95
#, python-brace-format
msgid "The overloards have deleted the tournament: {0} because there are not enough players left!"
msgstr "Володарі видалили турнір {0}, оскільки не залишилося достатньо гравців!"

#: chat/serializers.py:99
#, python-brace-format
msgid Game is due! {0} -> {1} -> @{2} vs @{3} Go to the game lobby to not miss the game! Remember the overloards are not very patient!"
msgstr "Час гри настав! {0} -> {1} -> @{2} проти @{3}. Перейдіть до лобі гри, щоб не пропустити матч! Пам’ятайте, володарі не дуже терплячі!"

#: chat/serializers.py:178
msgid "System"
msgstr "Система"

#: chat/utils.py:28
msgid "You are not a member of this conversation"
msgstr "Ви не є учасником цієї розмови"

#: chat/views.py:39
msgid "No conversations found..."
msgstr "Розмов не знайдено..."

#: chat/views.py:40
msgid "Conversations loaded successfully"
msgstr "Розмови успішно завантажено"

#: chat/views.py:56
msgid "Invalid message ID for infinite scroll"
msgstr "Недійсний ідентифікатор повідомлення для безкінечного прокручування"

#: chat/views.py:58 chat/views.py:78
msgid "No more messages to load"
msgstr "Більше немає повідомлень для завантаження"

#: chat/views.py:70
msgid "Message not found"
msgstr "Повідомлення не знайдено"

#: chat/views.py:90
msgid "We know that you haven't seen the messages below..."
msgstr "Ми знаємо, що ви ще не бачили нижче наведені повідомлення..."

#: chat/views.py:96
msgid "This message is hidden because you are blocking the user"
msgstr "Це повідомлення приховано, оскільки ви заблокували цього користувача"

#: chat/views.py:118
msgid "Messages loaded successfully"
msgstr "Повідомлення успішно завантажено"

#: chat/views.py:128
msgid "You can't use commands in the initial message"
msgstr "Ви не можете використовувати команди в першому повідомленні"

#: chat/views.py:130
msgid "No 'userId' provided"
msgstr "Не вказано 'userId'"

#: chat/views.py:132
msgid "No 'initialMessage' provided"
msgstr "Не вказано 'initialMessage'"

#: chat/views.py:135
msgid "You are blocked by the user"
msgstr "Цей користувач заблокував вас"

#: chat/views.py:146
msgid "Conversation created (or found) and delivered initial message."
msgstr "Розмову створено (або знайдено) та надіслано початкове повідомлення."

