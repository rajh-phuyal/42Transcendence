msgstr "a chave '{key}' é obrigatória no tipo de mensagem '{message_type}'"

#: tournament/utils.py:27
msgid "Can't create a tournament without a name"
msgstr "Não é possível criar um torneio sem um nome"

#: tournament/utils.py:32
msgid "Tournament name can only contain letters, numbers, hyphens (-), and underscores (_)"
msgstr "O nome do torneio só pode conter letras, números, hífenes (-) e traços inferiores (_)"

#: tournament/utils.py:34
#, python-brace-format
msgid "Tournament name cannot be longer than {MAX_LENGHT_OF_TOURNAMENT_NAME} characters"
msgstr "O nome do torneio não pode ter mais do que {MAX_LENGHT_OF_TOURNAMENT_NAME} caracteres"

#: tournament/utils.py:36
msgid "Can't create a tournament without specifying a map number"
msgstr "Não é possível criar um torneio sem especificar um número de mapa"

#: tournament/utils.py:38
msgid "Invalid map number"
msgstr "Número de mapa inválido"

#: tournament/utils.py:50
msgid "You can't create a new tournament while you are in another one"
msgstr "Não podes criar um novo torneio enquanto estás noutro"

#: tournament/utils.py:61
msgid "Private tournaments require opponent ids"
msgstr "Torneios privados requerem IDs de adversários"

#: tournament/utils.py:64
msgid "Public tournaments can't have opponent ids"
msgstr "Torneios públicos não podem ter IDs de adversários"

#: tournament/utils.py:68
msgid "Opponent ids must be a list"
msgstr "Os IDs dos adversários devem ser uma lista"

#: tournament/utils.py:70
msgid "You must invite at least two opponents to a tournament"
msgstr "Deves convidar pelo menos dois adversários para um torneio"

