msgstr "Jogo já existe: {link_game}"

#: chat/serializers.py:27
#, python-brace-format
msgid "@{0} has invited @{1} to play a friendly match: {2}"
msgstr "@{0} convidou @{1} para jogar uma partida amigável: {2}"

#: chat/serializers.py:31
#, python-brace-format
msgid "@{0} has deleted the friendly match: {1}"
msgstr "@{0} excluiu a partida amigável: {1}"

#: chat/serializers.py:35
#, python-brace-format
msgid "@{0} has conceded the friendly match {1} and is therefore the loser"
msgstr "@{0} desistiu da partida amigável {1} e é, portanto, o perdedor"

#: chat/serializers.py:39
#, python-brace-format
msgid "@1 lost their patience. Game {0} has been set to finished since the deadline has passed. They randomly decided a winner."
msgstr "@1 perdeu a paciência. O jogo {0} foi dado como finalizado, pois o prazo expirou. Eles decidiram um vencedor aleatoriamente."

#: chat/serializers.py:43
#, python-brace-format
msgid "User @{0} has sent a friend request to @{1}"
msgstr "O usuário @{0} enviou uma solicitação de amizade para @{1}"

#: chat/serializers.py:47
#, python-brace-format
msgid "User @{0} has accepted the friend request from @{1}"
msgstr "O usuário @{0} aceitou a solicitação de amizade de @{1}"

#: chat/serializers.py:51
#, python-brace-format
msgid "User @{0} has canceled the friend request to @{1}"
msgstr "O usuário @{0} cancelou a solicitação de amizade para @{1}"

#: chat/serializers.py:55
#, python-brace-format
msgid "User @{0} has rejected the friend request from @{1}"
msgstr "O usuário @{0} rejeitou a solicitação de amizade de @{1}"

#: chat/serializers.py:59
#, python-brace-format
msgid "User @{0} has removed @{1} from their friends list"
msgstr "O usuário @{0} removeu @{1} da lista de amigos"

#: chat/serializers.py:63
#, python-brace-format
msgid "User @{0} has blocked @{1}"
msgstr "O usuário @{0} bloqueou @{1}"

#: chat/serializers.py:67
#, python-brace-format
msgid "User @{0} has unblocked @{1}"
msgstr "O usuário @{0} desbloqueou @{1}"

#: chat/serializers.py:71
#, python-brace-format
