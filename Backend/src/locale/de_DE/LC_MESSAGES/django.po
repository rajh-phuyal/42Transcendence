#, python-brace-format
msgid "User @{0} has blocked @{1}"
msgstr "Benutzer @{0} hat @{1} blockiert"

#: chat/serializers.py:67
#, python-brace-format
msgid "User @{0} has unblocked @{1}"
msgstr "Benutzer @{0} hat @{1} entblockt"

#: chat/serializers.py:71
